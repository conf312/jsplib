<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jsplib.users.book.dao.BookMapper">
    <select id="selectBookList" resultType="com.jsplib.users.book.dao.BookEntity">
        select
            idx,
            title,
            isbn,
            author,
            year_issue,
            thumbnail_url
        from
            book
    </select>
</mapper>